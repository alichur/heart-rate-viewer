{
  "name": "minimalReact",
  "version": "0.0.1",
  "description": "write the most basic hello world I could make with React",
  "dependencies": {
    "express": "^4.17.1",
    "isomorphic-fetch": "^2.2.1",
    "npm": "^6.9.0",
    "react": "^16.8.6",
    "react-redux": "^5.1.1",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-auth-wrapper": "^1.1.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "babelify": "^7.3.0",
    "browserify": "^16.2.3",
    "chai": "^3.5.0",
    "enzyme": "^2.9.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "jsdom": "^9.12.0",
    "livereactload": "^3.5.0",
    "mocha": "^6.1.4",
    "nock": "^9.6.1",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-proxy": "^1.1.8",
    "react-router": "^3.2.1",
    "redux-mock-store": "^1.5.3",
    "semistandard": "^9.1.0",
    "sinon": "^1.17.7",
    "watchify": "^3.11.1"
  },
  "scripts": {
    "watch": "watchify -t [ babelify --presets [ react ] ] -p [ livereactload --port 4475 ] src/main.jsx -o public/build/bundle.js -v",
    
    "build": "browserify -t [ babelify --presets [ react ] ] src/main.jsx -o public/build/bundle.js",
    "debug": "browserify --debug -t [ babelify --presets [ react ] ] src/main.jsx -o public/build/bundle.js",
    "test": "mocha --compilers js:babel-register --require test/helpers.js --require test/dom.js --recursive",
    "test-debug": "mocha --compilers js:babel-register --require test/helpers.js --require test/dom.js --recursive --debug-brk",
    "lint": "./node_modules/.bin/eslint src"
  }
}